#include<stdio.h>

int strcmp(char* str1, char* str2){
	if (sizeof(str1)!=sizeof(str2)) return 0;
	else {
		int i;
		for(i=0;str1[i]!='\0';i++)
			if (str1[i]!=str2[i]) return 0;
	}
	return 1;
}

int main(void){
	char* str1="000000";
	char* str2="000010";
	int i=strcmp(str1,str2);
	printf("%d\n",i);
}

