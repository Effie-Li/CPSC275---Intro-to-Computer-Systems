#include<stdio.h>

int strcmp(char* str1, char* str2){
	if (sizeof(str1)!=sizeof(str2)) return 0;
	else {
		int i;
		for(i=0;str1[i]!='\0';i++)
			if (str1[i]!=str2[i]) return 0;
	}
	return 1;
}


char hexLetter(char* bits){
	if (strcmp(bits,"0000")) return '0';
	else if (strcmp(bits,"0001"))	return '1';
	else if (strcmp(bits,"0010"))	return '2';
	else if (strcmp(bits,"0011"))	return '3';
	else if (strcmp(bits,"0100"))   return '4';
	else if (strcmp(bits,"0101"))	return '5';
	else if (strcmp(bits,"0110"))	return '6';
	else if (strcmp(bits,"0111"))	return '7';
	else if (strcmp(bits,"1000"))	return '8';
	else if (strcmp(bits,"1001"))	return '9';
	else if (strcmp(bits,"1010"))	return 'A';
	else if (strcmp(bits,"1011"))	return 'B';
	else if (strcmp(bits,"1100"))	return 'C';
	else if (strcmp(bits,"1101"))	return 'D';
	else if (strcmp(bits,"1110"))	return 'E';
	else if (strcmp(bits,"1111"))	return 'F';
}

int main(void){
	char c = hexLetter("0011");
	printf("%c\n",c);
}
